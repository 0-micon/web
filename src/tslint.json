{
  "extends": "../tslint.json",
  "rules": {
    "variable-name": false,
    "no-inferrable-types": [
      false,
      "ignore-params"
    ],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
